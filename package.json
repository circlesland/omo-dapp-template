{
  "name": "omo-dapp-template",
  "version": "0.0.1",
  "description": "snowpack, svelte, typescript, tailwind, fission dapp template",
  "main": "src/index.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/omoearth/omo-dapp-template.git"
  },
  "keywords": [
    "snowpack",
    "svelte",
    "typescript",
    "tailwind",
    "fission"
  ],
  "author": "MamaOmo",
  "license": "AGPL-3.0",
  "bugs": {
    "url": "https://github.com/omoearth/omo-dapp-template/issues"
  },
  "homepage": "https://github.com/omoearth/omo-dapp-template#readme",
  "scripts": {
    "clean:dist": "npx shx rm -rf build",
    "clean:modules": "npx shx rm -rf node_modules",
    "clean": "npm run clean:dist && npm run clean:modules",
    "build:clean": "npm run clean:all && npm run build",
    "build:clean:dist": "npm run clean:dist && npm run build",
    "dev": "snowpack dev",
    "build": "npm install && snowpack build",
    "deploy": "npm run build && fission app publish --verbose"
  },
  "dependencies": {
    "svelte": "^3.30.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-transform-runtime": "^7.12.1",
    "@babel/preset-env": "^7.12.7",
    "@snowpack/plugin-babel": "^2.1.4",
    "@snowpack/plugin-dotenv": "^2.0.4",
    "@snowpack/plugin-postcss": "^1.0.7",
    "@snowpack/plugin-svelte": "^3.3.0",
    "@snowpack/plugin-typescript": "^1.1.0",
    "@snowpack/plugin-webpack": "^2.1.1",
    "autoprefixer": "^10.0.2",
    "cssnano": "^4.1.10",
    "postcss": "^8.1.10",
    "postcss-cli": "^8.3.0",
    "postcss-preset-env": "^6.7.0",
    "snowpack": "^2.17.1",
    "svelte-preprocess": "^4.0.8",
    "tailwindcss": "^2.0.1",
    "typescript": "^4.1.2"
  }
}
